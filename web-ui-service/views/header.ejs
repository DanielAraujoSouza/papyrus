<div class="search-wrapper">
  <% if (title != "Home") { %> 
  <div class="header-input">
    <input id="header-searchbar" type="text" name="search" placeholder="Buscar...">
    <span id="searchbar-icon" class="mdi mdi-magnify"></span>
  </div>
  <% } %> 
</div>
<div class="accessibility-wrapper">
  <a class="contrast" href="#">
    <span class="mdi mdi-contrast-circle icon"></span>
  </a>
  <a class="font-size-increase" href="#">
    <span class="mdi mdi-format-font-size-increase icon"></span>
  </a>
  <a class="font-size-decrease" href="#">
    <span class="mdi mdi-format-font-size-decrease icon"></span>
  </a>
</div>

<%# Se estiver logado %> 
<% if (locals.user) { %>
  <div class="dropdown">
    
    <% if(user.avatar_path) {%>
      <div class="dropbtn user-navbar-icon-wrapper">
        <img class="user-navbar-icon" src="<%= user.avatar_path %>">
      </div>
    <% } else { %>
      <div class="dropbtn">
        <svg width="28" height="30" class="position-user-img">
          <circle r="14" cx="14" cy="16"/>
            <text
              x="14"
              y="22"
              text-anchor="middle"
              stroke-width="0.5px"
              font-family="Source Sans Pro, sans-serif"
              font-size="18px"
            >
            <%= user.name[0].toUpperCase() %> 
            </text>
        </svg>
      </div>
    <% } %>
    <div class="dropdown-content">
      <div class="user-name">
        <span id="dropdown-user-name">
          <%= user.name.slice(0,15) %> 
        </span>
      </div>
      <div class="user-options">
        <a  href="/user/profile">
          <span class="mdi mdi-account"></span>
          Meu Perfil</a>
        <a href="/user/favorites">
          <span class="mdi mdi-cards-heart"></span>
          Favoritos
        </a>
      </div>
      <% if(user.role === "ADMIN") { %> 
      <div class="admin-options">
        <a href="/book/insert">
          <span class="mdi mdi-book-open-blank-variant"></span>
          Adicionar Livro
        </a>
        <a href="/ebook/insert">
          <span class="mdi mdi-tablet-cellphone"></span>
          Adicionar Ebook
        </a>
        <a href="/author/insert">
          <span class="mdi mdi-account-box"></span>
          Adicionar Autor
        </a>
      </div>
      <% } %> 
      <div class="logout">
        <a href="/user/sigout">
          <span class="mdi mdi-exit-run"></span>
          Sair
        </a>
      </div>
    </div>
  </div>

  <%# Se nãoestiver logado %> 
  <%} else { %> 
  <button class="open-button" id="btn-entrar"> 
    <span class="mdi mdi-account icon2"></span>
    <span>Entrar</span>
  </button>
  <div class="form-popup" id="loginForm-wrapper">
    <form class="form-login" id="form-login" >
      
      <input id="loginEmail" type="email" placeholder="Digite seu e-mail" name="userEmail" required>
      <input id="loginPassword" type="password" placeholder="Digite a Senha" name="userPassword" required>

      <button id="loginBtn" type="submit" class="btn">
        <span id="loadLoginIcon" class="mdi mdi-loading mdi-spin loading"></span>
        <span id="btnLoginText">Entrar</span>
      </button>
      <label class="erro" id="login-erro"></label>
    </form>
    <div class="line-division">
      <a href="/registration">Ainda não tem conta? Cadastre-se aqui</a>
    </div>
  </div>
<% } %>